# API Configuration
VITE_API_URL=http://localhost:3011

# Data Source Configuration
# Available options: local, rest, graphql
VITE_DATA_SOURCE_TYPE=rest

# Storage Configuration
VITE_STORAGE_PREFIX=app_
VITE_STORAGE_VERSION=1.0

# Auth Configuration
VITE_AUTH_TOKEN_KEY=your-very-secret-key-for-access-token
VITE_AUTH_REFRESH_TOKEN_KEY=your_secure_refresh_secret_key

REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id
REACT_APP_GITHUB_CLIENT_ID=your-github-client-id

# API Endpoints (for REST and GraphQL)
VITE_API_ENDPOINT_AUTH=/auth
VITE_API_ENDPOINT_USERS=/users
VITE_API_ENDPOINT_DASHBOARDS=/dashboards
VITE_API_ENDPOINT_CARDS=/cards
VITE_API_ENDPOINT_GRAPHQL=/graphql